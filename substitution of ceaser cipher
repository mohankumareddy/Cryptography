        if (ciphertext[i] >= 32 && ciphertext[i] <= 127) {
            plaintext[i] = key[(int)ciphertext[i] - 32];
        } else {
            plaintext[i] = ciphertext[i];
        }
    }
    plaintext[i] = '\0'; 

    printf("Decrypted plaintext: %s\n", plaintext);

